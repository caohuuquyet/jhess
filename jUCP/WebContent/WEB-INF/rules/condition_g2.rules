@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix owl:	<http://www.w3.org/2002/07/owl#>.
@prefix jhess: <http://jhess.googlecode.com/files/jhess.owl#>.


[jhess1: 
 (jhess:d01_meter_entrance rdf:type ?d01)
 (jhess:d01_meter_entrance jhess:hasInputPower ?a1)

  noValue(?s jhess:ruleFiredFor1 jhess:SemanticWeb1)
 ->
 remove(1)

 (jhess:d01_meter_entrance jhess:hasInputPower 0)

 (?s jhess:ruleFiredFor1 jhess:SemanticWeb1)
 hide(jhess:ruleFiredFor1)
 ]
 
 [jhess2: 
 (jhess:d02_lightfitting_entrance rdf:type ?d02)
 (jhess:d02_lightfitting_entrance jhess:hasInputPower ?a2)
 (jhess:d02_lightfitting_entrance jhess:hasDescription ?b2)
 (jhess:d02_lightfitting_entrance jhess:hasStatusStartTime ?c2)
 noValue(?s jhess:ruleFiredFor2 jhess:SemanticWeb2)
  ->
 remove(1)
 remove(2) remove(3)
 (jhess:d02_lightfitting_entrance jhess:hasInputPower 0)
 (jhess:d02_lightfitting_entrance jhess:hasDescription "")
 (jhess:d02_lightfitting_entrance jhess:hasStatusStartTime "")
 (?s jhess:ruleFiredFor2 jhess:SemanticWeb2)
 hide(jhess:ruleFiredFor2)
 ]
 
  
 
