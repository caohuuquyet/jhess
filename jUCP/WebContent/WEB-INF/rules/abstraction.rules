@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix owl:	<http://www.w3.org/2002/07/owl#>.
@prefix jhess: <http://jhess.googlecode.com/files/jhess.owl#>.


[layer1: 
(?d jhess:hasDescription "Main meter"^^xsd:string),
(?d jhess:hasLocation jhess:room_4688_entrance),
(?d jhess:hasInputPower 10),
(?d jhess:hasInputPowerUnit "W"^^xsd:string),
(?d jhess:hasCurrentDeviceStatus "ON"^^xsd:string),
(?d jhess:hasStatusStartTime "2013-01-12T21:30:52"^^xsd:dateTime),	   
(?d jhess:hasCurrentMeasureValue 100),
(?d jhess:hasMeasureUnit "W"^^xsd:string),
(?d jhess:isMeasuringFor jhess:hess),	   
(?d jhess:hasHistoryData jhess:cloud_of_data),
noValue(?c jhess:ruleFiredFora jhess:jhessv) 
->
remove(2),
(?c jhess:ruleFiredFora jhess:jhessv),	
hide(jhess:ruleFiredFora)
]

[jhess1: 
(jhess:d12_sensor_temperature jhess:hasCurrentTemperatureValue ?value ), 
greaterThan(?value,25), 
(jhess:d11_heating_kitchenette jhess:hasCurrentDeviceStatus "ON"^^xsd:string ), 
noValue(?c jhess:ruleFiredFora jhess:jhessv) 
-> 
remove(2), 
(jhess:d11_heating_kitchenette jhess:hasCurrentDeviceStatus "OFF"^^xsd:string ), 
(?c jhess:ruleFiredFora jhess:jhessv),	
hide(jhess:ruleFiredFora)
]