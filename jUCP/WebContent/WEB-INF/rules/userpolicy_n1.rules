@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix owl:	<http://www.w3.org/2002/07/owl#>.
@prefix jhess: <http://jhess.googlecode.com/files/jhess.owl#>.


[jhess1: 
 (jhess:d01_meter_entrance rdf:type ?d01)
 (jhess:d01_meter_entrance jhess:hasCurrentDeviceStatus "OFF"^^xsd:string)

  noValue(?s jhess:ruleFiredFor1 jhess:SemanticWeb1)
 ->
 remove(1)

 (jhess:d01_meter_entrance jhess:hasCurrentDeviceStatus "")

 (?s jhess:ruleFiredFor1 jhess:SemanticWeb1)
 hide(jhess:ruleFiredFor1)
 ]
 
