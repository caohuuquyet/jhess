<urn:swrl#x> rdf:type swrl:Variable .
[ rdf:type swrl:Imp ;
  swrl:body [ rdf:type swrl:AtomList ;
              rdf:first [ rdf:type swrl:ClassAtom ;
                          swrl:classPredicate :LightingDevice ;
                          swrl:argument1 <urn:swrl#x>
                        ] ;
              rdf:rest [ rdf:type swrl:AtomList ;
                         rdf:rest rdf:nil ;
                         rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                     swrl:argument2 "OFF"^^xsd:string ;
                                     swrl:propertyPredicate :hasValue ;
                                     swrl:argument1 <urn:swrl#x>
                                   ]
                       ]
            ] ;
  swrl:head [ rdf:type swrl:AtomList ;
              rdf:rest rdf:nil ;
              rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                          swrl:argument2 "ON"^^xsd:string ;
                          swrl:propertyPredicate :hasValue ;
                          swrl:argument1 <urn:swrl#x>
                        ]
            ]
] .

