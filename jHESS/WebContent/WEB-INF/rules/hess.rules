@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:	<http://www.w3.org/2002/07/owl#> .


@prefix : <http://www.hess.tsp.eu/2013/1/Maisel.owl#> .

[VentOFF:
	( ?humidity	rdf:type :HumiditySensor )
	( ?humidity	:hasCurrentHumidityValue ?value )
	greaterThan(?value, 20)
	( ?vent	rdf:type :Vent )
	( ?vent	:hasCurrentDeviceStatus "ON"^^xsd:string )
	noValue(?c :ruleFiredFor :jhess)
->
	remove(4)   
	(?vent	:hasCurrentDeviceStatus "OFF"^^xsd:string )
	(?c :ruleFiredFor :jhess)
	hide(:ruleFiredFor)	
	
]
