#################################################################
#    Classes
#################################################################

### Device
jhess:Device a owl:Class.

jhess:Meter rdfs:subClassOf jhess:Device.

jhess:Sensor rdfs:subClassOf jhess:Device.
jhess:PresenceSensor rdfs:subClassOf jhess:Sensor. 
jhess:HumiditySensor rdfs:subClassOf jhess:Sensor.
jhess:TemperatureSensor rdfs:subClassOf jhess:Sensor.

jhess:LightingDevice rdfs:subClassOf jhess:Device.

jhess:Vent rdfs:subClassOf jhess:Device.

jhess:HeatingDevice rdfs:subClassOf jhess:Device.

jhess:ComputingDevice rdfs:subClassOf jhess:Device.
jhess:Laptop rdfs:subClassOf jhess:ComputingDevice.
jhess:iPad rdfs:subClassOf jhess:ComputingDevice.

jhess:Refrigerator rdfs:subClassOf jhess:Device.

### Location
jhess:Location a owl:Class.
 
### Resident
jhess:Resident a owl:Class.

### CloudOfData
jhess:DataCloud a owl:Class. 

################################################################# 
#    Properties
#################################################################
# for devices
jhess:hasDescription a owl:DatatypeProperty;                   
        rdfs:domain jhess:Device;
        rdfs:range xsd:string.
        
jhess:hasLocation a owl:ObjectProperty ;                   
        rdfs:domain jhess:Device;
        rdfs:range jhess:Location.
		
jhess:hasInputPower a owl:DatatypeProperty;
	    rdfs:domain jhess:Device;
		rdfs:range xsd:int.
		
jhess:hasInputPowerUnit a owl:DatatypeProperty;
	    rdfs:domain jhess:Device;
		rdfs:range xsd:string.

jhess:hasCurrentDeviceStatus a owl:DatatypeProperty;
	    rdfs:domain jhess:Device;
		rdfs:range xsd:string.
		
jhess:hasStatusStartTime a owl:DatatypeProperty;
	    rdfs:domain jhess:Device;
		rdfs:range xsd:dateTime.

jhess:hasHistoryData a owl:ObjectProperty ;                   
        rdfs:domain jhess:Device;
        rdfs:range jhess:DataCloud.

# for meters
jhess:hasCurrentMeasureValue a owl:DatatypeProperty;
	    rdfs:domain jhess:Meter;
		rdfs:range xsd:int.
	
jhess:hasMeasureUnit a owl:DatatypeProperty;
	    rdfs:domain jhess:Meter;
		rdfs:range xsd:string.
		
jhess:isMeasuringFor a owl:ObjectProperty ;                   
        rdfs:domain jhess:Meter;
        rdfs:range jhess:Resident.
		
jhess:hasMeasureStartTime a owl:DatatypeProperty ;                   
        rdfs:domain jhess:Meter;
        rdfs:range xsd:dateTime.
		
# for temperature sensors
jhess:hasCurrentTemperatureValue a owl:DatatypeProperty ;                   
        rdfs:domain jhess:TemperatureSensor;
        rdfs:range xsd:int.
		
jhess:hasTemperatureUnit a owl:DatatypeProperty ;                   
        rdfs:domain jhess:TemperatureSensor;
        rdfs:range xsd:string.		

# for humidity sensors
jhess:hasCurrentHumidityValue a owl:DatatypeProperty ;                   
        rdfs:domain jhess:HumiditySensor;	
		rdfs:range xsd:int.	

jhess:hasHumidityUnit a owl:DatatypeProperty;
	    rdfs:domain jhess:HumiditySensor;
		rdfs:range xsd:string.
		
# for presence sensors
jhess:hasCurrentPresenceValue a owl:DatatypeProperty ;                 
        rdfs:domain jhess:PresenceSensor;
		rdfs:range xsd:boolean.	   

# for locations
jhess:isLocatedIn a owl:ObjectProperty ;                   
        rdfs:domain jhess:Location;
        rdfs:range jhess:Location.
jhess:hasTemperature a owl:ObjectProperty ;                   
        rdfs:domain jhess:Location;
        rdfs:range jhess:TemperatureSensor.
jhess:hasHumidity a owl:ObjectProperty ;                   
        rdfs:domain jhess:Location;
        rdfs:range jhess:HumiditySensor.
jhess:hasPresence a owl:ObjectProperty ;                   
        rdfs:domain jhess:Location;
        rdfs:range jhess:PresenceSensor.
		
