#WHERE { 
#?id :hasLocation ?location. ?id :hasInputPower ?inputpower. ?id :hasInputPowerUnit  ?unit. ?id :hasCurrentDeviceStatus ?status.?id :hasStatusStartTime ?startime. ?id :hasHistoryData ?datacloud.
#
#FILTER (?inputpower > 0)
#}
#ORDER BY ASC(?id)


